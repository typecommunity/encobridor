{
    "name": "autostacker/cloaker-pro",
    "description": "Professional Cloaking System - AutoStacker Edition",
    "type": "project",
    "license": "proprietary",
    "version": "1.0.0",
    "authors": [
        {
            "name": "AutoStacker Team",
            "email": "support@autostacker.app"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^7.0",
        "mobiledetect/mobiledetectlib": "^2.8",
        "jaybizzle/crawler-detect": "^1.2",
        "geoip2/geoip2": "^2.13",
        "monolog/monolog": "^2.0",
        "vlucas/phpdotenv": "^5.5",
        "firebase/php-jwt": "^6.0",
        "predis/predis": "^2.0",
        "phpmailer/phpmailer": "^6.8",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5",
        "fakerphp/faker": "^1.21",
        "symfony/var-dumper": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "CloakerPro\\": "core/",
            "CloakerPro\\Admin\\": "admin/",
            "CloakerPro\\Api\\": "api/"
        },
        "files": [
            "core/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php install.php"
        ],
        "test": [
            "@php vendor/bin/phpunit"
        ],
        "clear-cache": [
            "@php -r \"array_map('unlink', glob('storage/cache/*.*'));\""
        ],
        "generate-key": [
            "@php -r \"echo 'APP_KEY=' . base64_encode(random_bytes(32)) . PHP_EOL;\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    },
    "extra": {
        "installer-paths": {
            "vendor/{$vendor}/{$name}/": ["type:library"]
        }
    }
}